{
  "presets": ["@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@src": "./src",
          "@assets": "./src/assets",
          "@components": "./src/components",
          "@styles": "./src/styles"
        },
        "extensions": [".js", ".jsx"]
      }
    ]
  ]
}
